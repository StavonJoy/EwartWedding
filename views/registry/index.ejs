<%- include('../partials/header') %>
<h1>Registry page</h1>

<% if (user.host === true) { %>
<p>You are the host, you can add things</p>
<% } %>



<% items.forEach(item => { %>
  <div class="card">
    <div class="card-header">
      <%= item.name %>
    </div>
    <div class="card-body">
      <p>Price: <%= item.price %></p>
      <a href="">Amazon Link</a>
    </div>
  </div>
<% }) %>

<%- include('../partials/footer') %>